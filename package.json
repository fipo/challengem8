{
  "name": "keto-challenge.com",
  "version": "1.0.0",
  "description": "14 Day Keto Challenge App",
  "private": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prestart": "firebase functions:config:get > .firebase-config.json",
    "start": "webpack-dev-server --mode development --color --progress --config webpack.dev.js",
    "build": "webpack --mode production --color --progress --config webpack.prod.js",
    "predeploy": "yarn build",
    "deploy": "firebase deploy",
    "deploy:functions": "firebase deploy --only functions",
    "postdeploy": "rimraf ./dist"
  },
  "author": "Philip Sotirov | @fipo",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.1.5",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.1.5",
    "@babel/preset-react": "^7.0.0",
    "@firebase/app-types": "^0.3.2",
    "@firebase/util": "^0.2.6",
    "@types/react": "^16.7.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "clean-webpack-plugin": "^1.0.1",
    "css-loader": "^2.1.0",
    "eslint": "^5.14.1",
    "eslint-config-prettier": "^4.0.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.10.0",
    "handlebars": "^4.1.0",
    "handlebars-loader": "^1.7.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.0",
    "lint-staged": "^8.1.0",
    "prettier": "1.16.1",
    "redux-devtools-extension": "^2.13.5",
    "rimraf": "^2.6.2",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.5",
    "webpack-bundle-analyzer": "^3.0.4",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.0",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
    "firebase": "^5.5.8",
    "moment": "^2.22.2",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.1.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint 'src/**/*.js' --fix",
      "git add"
    ]
  }
}
